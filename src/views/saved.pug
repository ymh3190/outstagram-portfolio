extends layouts/main

block content
    include ./partials/mainHeader
    include ./partials/mainNav
    div
        article
            div
                each save in loggedUser.saves
                    div
                        div
                            if save.type === 'image'
                                img(src="/"+save.url, alt="post" style="width:400px; height:400px; object-fit: fill;")
                            else if save.type === 'video'
                                video(src="/"+save.url style="width:400px; height:400px; object-fit: fill;" controls) 
